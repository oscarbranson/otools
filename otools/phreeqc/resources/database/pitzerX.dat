	Hfo_wOH + Mg+2 = Hfo_wOMg+ + H+
	log_k -4.6
#   Manganese
	Hfo_sOH + Mn+2 = Hfo_sOMn+ + H+
	log_k  -0.4		     # table 10.5

	Hfo_wOH + Mn+2 = Hfo_wOMn+ + H+
	log_k -3.5		      # table 10.5
